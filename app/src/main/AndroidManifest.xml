<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ico_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ico_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoinquyLife"
        tools:targetApi="31" >

        <activity android:name=".Auth.SplashActivity"
            android:exported="true">
        </activity>

        <activity
            android:name=".Auth.UI.AuthActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".SelectionHouse.UI.CoinquyHouseSelectionActivity"
            android:exported="true">
        </activity>

        <activity
            android:name=".dashboard.UI.DashboardActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".setting.SettingActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".setting.ProfileActivity" />
        </activity>

        <activity
            android:name=".setting.ProfileActivity"
            android:exported="true" />
    </application>

</manifest>